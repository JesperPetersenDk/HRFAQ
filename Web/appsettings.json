{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=hrfaq;Trusted_Connection=True;"
  },
  "SettingInformation": {
    "TargetBlankLink": true
  },
  "AllowedHosts": "*",
  "Serilog": {
    "LogMode": 3, // 1: File, 2: Database, 3: Both
    "File": {
      "Path": "logs/log.txt",
      "RollingInterval": "Day"
    },
    "DatabaseSerilog": {
      "TableName": "Logs"
    }
  }
}
