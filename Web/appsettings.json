{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=hrfaq;Trusted_Connection=True;"
  },
  "SettingInformation": {
    "TargetBlankLink": true,
    "RemoveMatchWord": false,
    "MulitiAnswer": true,
    "StatusRapport": true,
    "Categori": false
  },
  "LettersSetting": {
    "count": "35",
    "after": "..."
  },
  "AllowedHosts": "*",
  "Serilog": {
    "LogMode": 3, // 1: File, 2: Database, 3: Both
    "File": {
      "Path": "logs/log.txt",
      "RollingInterval": "Day"
    },
    "DatabaseSerilog": {
      "TableName": "Logs"
    }
  }
}
